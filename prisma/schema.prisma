generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
 
 model taks{
 id int @id @default(autoincrement())
 titulo string @db.varchar(100)
 descricao string @db.varchar(100)
 }